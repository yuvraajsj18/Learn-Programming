start:          INP
                STA A 0
                INP
                STA B 0
                CLA
                INC
                CMA
                INC
                STA MINUS 0
CHECK:          LDA B 0
                SZA
                BUN MULTIPLY 0
                LDA PRODUCT 0
                OUT
                HLT
MULTIPLY:       LDA MINUS 0
                ADD B 0
                STA B 0
                LDA A 0
                ADD PRODUCT 0
                STA PRODUCT 0
                BUN CHECK 0
A:              .data 1 0
B:              .data 1 0
MINUS:          .data 1 0
PRODUCT:        .data 1 0