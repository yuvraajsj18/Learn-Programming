start:      LDA actualadd   ; 298 is actual add of operand. load it in ac
            STA 0x082     ; store this add at memory loc 082
            LDA num1  ; 632 is operand, load it in ac
            STA_I 0x082     ; store operand at loc 298
            LDA num2      ; load 937 in ac
            ADD_I 0x082     ; ADD 632 to ac
            STA num3      ; store result at loc designated by add num3
            OUT
            HLT
actualadd:  .data 1 0x298
num1:       .data 1 0x632
num2:       .data 1 0x937
num3:       .data 1 0