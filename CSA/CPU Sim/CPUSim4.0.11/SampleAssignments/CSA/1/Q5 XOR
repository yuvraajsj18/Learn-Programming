; XOR

start:      INP
            STA a
            INP
            STA b
            CMA
            AND a
            STA first
            LDA a
            CMA
            AND b
            STA second
            LDA first
            AND first
            CMA
            STA first
            LDA second
            AND second
            CMA
            STA second
            AND first
            CMA
            OUT
            HLT
            

a:          .data 1 0
b:          .data 1 0
first:      .data 1 0
second:     .data 1 0