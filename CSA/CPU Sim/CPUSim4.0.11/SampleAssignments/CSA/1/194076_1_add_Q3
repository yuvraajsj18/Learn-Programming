; Program to add two numbers

start:  INP     ; input first number
        STA  a   ; store the input in a
        INP     ; Take second input
        STA  b  ; store it into b
        ADD  a   ; add a to b that is in ac
        STA  sum ; store sum in sum
        OUT     ; sum of a and b is in ac output it
        HLT     ; end the program
a:      .data 1 0xA937
b:      .data 1 0x082
sum:    .data 1 0
